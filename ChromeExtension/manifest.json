{
  "name": "EyeEmotionPointing",
  "version": "1.0",
  "manifest_version": 2,
  "description": "pointing method using eye tracker and clmtrackr",
  "permissions": ["tabs", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "EyeEmotionPointing"
  }
}